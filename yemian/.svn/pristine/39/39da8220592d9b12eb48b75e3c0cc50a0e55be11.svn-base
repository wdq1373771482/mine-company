		 <script type="text/javascript">
			$(function(){
				//加载所有分类内容
				addList();
				
				//加载右上课程分类
				addRList();
			});
			function addList(){
				$.ajax({
					url:"assets/js/kclb.json",
					type:"POST",
					async:false,
					success:function(data){	
						var listLi="",
							listCont="";
						$.each(data,function(i,obj){
							listLi+= '<li><a href="#" target="_blank">' + obj.categoryName + '</a></li>';
							$.each(obj.children,function(ii,secObj){
								listCont+='<div class="list_c1"><h4><a href="#" target="_blank">' + secObj.categoryName + '</a></h4><p class="clearfix"><a href="#" target="_blank" class="a_1">全&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;部</a>';
								$.each(secObj.children,function(iii,thrObj){
									listCont+='<a href="#" target="_blank">' + thrObj.categoryName + '</a>';
								});
								listCont+='</p>';
								
							});
							listCont+='</div>';
						});	
						$(".list_cont ul").html(listLi);
						$(".list_contss").html(listCont);
					}
				});
			}
			function addRList(){
				$.ajax({
					url:"assets/js/kclb.json",
					type:"POST",
					async:false,
					dataType:"json",
					success:function(data){	
						var listLi='<a href="javascript:;"  class="cur1">全部</a>',
							listCont='';
						$.each(data,function(i,obj){
							listLi+= '<a href="javascript:;" >' + obj.categoryName + '</a>';
							listCont+='<div class="list_1 clearfix"><span></span><p><a href="javascript:;" class="cur">全部</a>';
							 $.each(obj.children,function(ii,secObj){
							
								 $.each(secObj.children,function(iii,thrObj){
								
									listCont+='<a href="#">' + thrObj.categoryName + '</a>'; console.log(thrObj.categoryName);
								 });		
							}); 
							listCont+='</p></div>';
						});	
						$(".xh_list > p").html(listLi);
						$(".listC").html(listCont);
					}
				});
			}
		 </script>
