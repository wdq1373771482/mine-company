@-webkit-keyframes bs {
  0% {
    box-shadow: inset -1px 1px 3px 5px #fff, inset 1px -1px 3px 2px #fff, 0 0 0px 0 #fff;
  }

  50% {
    box-shadow: inset -1px 1px 3px 5px #fff, inset 1px -1px 3px 2px #fff, 0 0 20px 0 #fff;
  }

  100% {
    box-shadow: inset -1px 1px 3px 5px #fff, inset 1px -1px 3px 2px #fff, 0 0 0px 0 #fff;
  }
}
@keyframes bs {
  0% {
    box-shadow: inset -1px 1px 3px 5px #fff, inset 1px -1px 3px 2px #fff, 0 0 10px 0 #fff;
  }

  50% {
    box-shadow: inset -1px 1px 3px 5px #fff, inset 1px -1px 3px 2px #fff, 0 0 20px 0 #fff;
  }

  100% {
    box-shadow: inset -1px 1px 3px 2px #fff, inset 1px -1px 3px 2px #fff, 0 0 0px 0 #fff;
  }
}
