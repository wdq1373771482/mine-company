	<script type="text/javascript">
		(function ($) {
			$(".hhh i").on("tap",function(){
				$(this).addClass("on1");
			});
			$(".hhh span").on("tap",function(){
				$(this).css({"background":"#e4c050","border":"none"});
			});
			$(".phoness").on("tap",function(){
				$(this).addClass("on1");
			});
			$(".alltxt2 dl").on("touchstart",function(){
				$(this).addClass("onn").siblings().removeClass("onn");
			});
			$(".footer li").on("tap",function(){
				var index = $(this).index();	
				$(this).addClass("curPage").siblings().removeClass("curPage");
			});
			$(document).on("tap",function(e){
				$(".licont ul").slideUp();
				$(".list_1 li").removeClass();
			});
			$(".list_1 li").on("tap",function(e){
				 e.stopPropagation();
			});
			$(".list_1  li").on("tap",function(){
				var ind=$(this).index();
				$(this).addClass("cur1").siblings().removeClass();
				$(".licont ul").eq(ind).slideDown().siblings().slideUp();
				$(".licont li").on("tap",function(){
					var text=$(this).text();
					var that=$(this);
					$(this).css("background","#fefefe");
					$(this).parent().find("a").css("color","#888");
					$(this).find("a").css("color","#ffb269");
					$(this).parent().slideUp(function(){
						that.css("background","#fff");
					});
					$(".list_1  li").each(function(){
						var _this=$(this);
						if(_this.hasClass("cur1")){
							_this.html(text);
						}
					});
				});
			});
			$(".licont li:last").off("tap");
		})(jQuery);
	</script>
