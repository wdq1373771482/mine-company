@media(min-width:414px){
	.bd li a span.span_r {
	  width: 72%;
	  position: relative;
	  margin-left: -20px;
	}
}

@media(min-width:375px){
	.progress i {
	  font-size: 1em;
	}
}

@media(max-width:360px){
	.bd li a span.span_r{
		width:63%;
	}
	.progress i{
		font-size:0.9em;
		right:-52px;
	}
}

@media(max-width:320px){
	.progress i{
		font-size:0.7em;
		right:-45px;
	}
}
@media(min-width:768px){
	.bd li a span.span_r{
		width:91%;
	}
	.progress i{
		font-size:1em;
		right:-63px;
	}
}















