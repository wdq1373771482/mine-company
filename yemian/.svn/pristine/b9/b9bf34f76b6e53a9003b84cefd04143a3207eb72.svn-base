body{
	background:#dcdcdc;
}
.header a:first-child span{
	 display: block;
  float: left;
  font-size: 1.2em;
  line-height: 4.5em;
}
.header a.dh{
	display:block;
	position:absolute;
	width:20px;
	height:60px;
	line-height:60px;
	color:#fff;
	font-weight:normal;
	top:0;
	right:0.5em;
	font-size:2em;
}
.phone{
	position: absolute;
	top: 18px;
	right: 0.8em;
	font-size: 2em;
	color: #fff;
	font-size:1.3em;
	display:none;
}
/* 3d */
.tt{
	background:#fff;
	padding: 2em 1em;
	text-indent: 0.4rem;
}
.tt span:first-child{
	font-size:2em;
	color:#5e5e5e;
}
.tt span:last-child{
	font-size:1.5em;
	color:#888;
}
.tt a{color:#098ac5;display:none;}
.tt.tt2 span{
	width:100%;
	padding:0.5em 0 0 0.5em;
}
.tt.tt2 span.bg em{
	height:2px;
	float:left;
	position:relative;
}
.tt.tt2 span.bg em.em1{
		background:#ff8479;
		width:24%;
}
.tt.tt2 span.bg em.em2{
		background:#ffcc00;
		width:30%;
}
.tt.tt2 span.bg em.em3{
		background:#40c69e;
		width:40%;
}
.tt.tt2 span.bg em i{
	display:block;
	padding:0em 0.5em;
	position:absolute;
	top:-10px;
	right:0;
	font-size:0.7em;
	color:#fff;
	border-radius:5px;
}
.em1 i{
	background:#ff8479;
}
.em2 i{
	background:#ffcc00;
}
.em3 i{
	background:#40c69e;
}
.tt.tt2 span:first-child img{
	width:100%;
}
.bd,.tt{
	margin:10px;
}
.bd{
	background:#fff;
	padding:0.5em 1em;
}
.bd li{
	MARGIN:1EM 0;		
}
.bd li a{
	display:block;
	color:#272727;
	font-size:1.5em;
	overflow:hidden;
	border-bottom:1px solid #eee
}
.bd span img{
	width:50%;
	display:block;
	margin-top:0.5em;
}
.bd li a span,.bd li a em{
	display:block;
	float:left;
}
.bd li a span.span_r{
	width:75%;
	position:relative;
	margin-left:-18px;
	margin-top:0%;
}
.bd li a em{
	line-height:3em;
}
.progress {
  height: 2px;
  position:absolute;
  width:90%;
  bottom:0;
  left:0;
  background:#eee;
  display:none;
 }
 .progress-bar-info {
  height: 2px;
}
 .progress1{
	background:#40c69e;
	width: 100%;
 }
 .progress2{
	background:#ffcc00;
	width: 50%;
 }
 .progress3{
	background:#ff8479;
	width: 25%;
 }
 .progress i{
   position: absolute;
  right: -62px;
  top: -11px;
	color:#ccc;
	font-size:0.6em;
	display:none;
 }
 .progresss1 i{
	COLOR:#40c69e;
 }
.progresss2 i{
	COLOR:#ffcc00;
 }
 .progresss3 i{
	COLOR:#ff8479;
 }
/* 蒙层 */
.black{
	filter:alpha(opacity=76);
	-moz-opacity:0.76;
	-khtml-opacity: 0.76;
	opacity: 0.76;
	background:#000;
	position:fixed;
	top:0;
	left:0;
	z-index:999999;
	display:none;
}
.contBox > div{
	width:250px;
	height:300px;
	position:fixed;
	left:50%;
	top:50%;
	margin-left:-125px;
	margin-top:-150px;
	z-index:9999999;
	display:none
}
