	<script type="text/javascript">
		(function($){
		
			//设置轮播图ul的宽
			var anLiW=(($(".pics").outerWidth(true))),
				liLen=$(".gallery_3lxx  li").length,
				WW=(anLiW*liLen);
				$('ul.gallery_3lxx').css({"width":(WW+100)+"px"});
			 var swiper = new Swiper('.swiper-container', {
					pagination: '.swiper-pagination',
					slidesPerView: 2.5,
					paginationClickable: true,
					spaceBetween: 3,
					freeMode: true
				});

			$(".phoness").on("tap",function(){
				$(this).addClass("on1");
			});
			$(".hhh span").on("tap",function(){
				$(this).css({"background":"#e4c050","border":"none"});
			});
			$(".footer li").on("tap",function(){
				var index = $(this).index();	
				$(this).addClass("curPage").siblings().removeClass("curPage");
			});
			
			//顶部更多标签
			$(".ul_1 li,.ul_111 li").on("tap",function(){
				$(this).addClass("on");
			});
			//
			var ttt2="收起热门标签",
				ttt1="更多热门标签";
			$(".p_center").toggle(function(){
				ch($(this));
				$(".bqCont").slideDown(450);
				$(".ul_11111").hide();
				$(this).html(ttt2);
			},function(){
				ch($(this));
				$(".bqCont").slideUp(450);
				$(".ul_11111").show();
				$(this).html(ttt1);
			});
			function ch(_this){
				_this.addClass("onnss");
				setTimeout(function(){
					_this.removeClass("onnss");
				},50);
			}
			
			//点击上一页，下一页
			$(".pageNum a").on("tap",function(){
				$(this).addClass("onnss");
				var _this=$(this);
				setTimeout(function(){
					_this.removeClass("onnss");
				},50);
			});
		})(jQuery);
	</script>
