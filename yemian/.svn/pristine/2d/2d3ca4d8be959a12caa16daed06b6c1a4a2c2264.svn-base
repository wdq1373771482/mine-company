body, div, p, h1, h2, h3, h4, h5, h6, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, table, th, td {
	margin : 0;
	padding : 0;
}
body, button, input, select, textarea {
	font : 12px / 24px "Hiragino Sans GB",
	"Hiragino Sans GB W3",
	"Microsoft YaHei",
	"微软雅黑",
	simsun,
	"宋体",
	tahoma,
	arial;
}
h1, h2, h3, h4, h5, h6 {
	font-size:100%;
	font-weight:normal;
}
address, cite, em, i {
	font-style:normal;
}
small {
	font-size : 12px;
}
li {
	list-style : none;
}
a {
	text-decoration : none;
	-webkit-tap-highlight-color : rgba(0, 0, 0, 0);
	-webkit-appearance : none;
	-moz-tap-highlight-color : rgba(0, 0, 0, 0);
	-moz-appearance : none;
	-o-tap-highlight-color : rgba(0, 0, 0, 0);
	-o-appearance : none;
	-ms-tap-highlight-color : rgba(0, 0, 0, 0);
	-ms-appearance : none;
	tap-highlight-color : rgba(0, 0, 0, 0);
	appearance : none;
	cursor:pointer;
}
a : hover {
	text-decoration : underline;
}
legend {
	color :  # 000;
}
fieldset, img {
	border : 0;
}
button, input, select, textarea {
	font-size : 100 % ;
	line-height : normal;
}
table {
	border-collapse : collapse;
	border-spacing : 0;
}
input[type="button"], input[type = "submit"], input[type = "reset"], a {
	/* cursor : pointer; */
}
a, input {
	outline : none;
}
.clearfix{zoom:1;}
.clearfix:after{display:block; content:'.'; clear:both; line-height:0; visibility:hidden;}
.mt10{margin-top:10px;}
.mt20{margin-top:20px;}
.mb10{margin-bottom:10px;}
.mb20{margin-bottom:20px;}
.p20{padding-top:20px;}
.fl{float:left}
.fr{float:right;}
.tl{text-align:left;}
.tr{text-align:right}
.tc{text-align:center;}
.green{
	color:#40c69e!important;
}
.hide{
	display:none;
}
body{
	font-family:"微软雅黑";
	 background:#f1f1f1;
}
li{
	list-style:none;
}
::-webkit-scrollbar { 
   display: none;
}
/* head */
.header{
    width:100%;
    z-index:99999;
    height:60px;
    background:#0692d3;
	position:relative;
}
.header h2{
	width:100%;
	text-align: center;
	color: #fff;
	position:relative;
	line-height:2.83em;
	font-size:1.5em;
	font-family:"微软雅黑";
	float:left;
	top:5px;
}
.header a:first-child{
	font-size:1.25em;
	position:absolute;
	left:0;
	top:0;
	z-index:99;
	color:#fff;
	height:60px;
	line-height:1.5em;
	background:url(../images/back.png) no-repeat center center;
	background-size:55% auto;
	width:30px;
}
.header a:first-child span{
	width: 40px;
	display: block;
	margin-top: 20px;
	margin-left: 25px;
	font-size:1.2em;
}
.header a.dh{
	display:block;
	position:absolute;
	line-height:38px;
	color:#fff;
	font-weight:normal;
	top:0;
	right:0.4em;
	font-size:3em;
	height:60px;
}
