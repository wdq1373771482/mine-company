			$(".list_1  li").on("click",function(){
				var ind=$(this).index();
				$(this).addClass("cur1").siblings().removeClass();
				if(ind != 3){
					$(".licont1 ul").eq(ind).slideDown().siblings().slideUp();
					$(".licont2 ul").css("left","100%");
					$('.licont').height("auto");
				}else{
					$(".licont1 ul").eq(ind).slideDown(function(){
						$(".zhuanye li").on("click",function(e){
							e.stopPropagation();
							var that=$(this);
							that.parent().stop();
							var curIndex=that.data("index");
							$(".licont2 ul").eq(curIndex).stop().animate({"left":"50%"},300).siblings().stop().animate({"left":"100%"},300);
						});
					}).siblings().slideUp();
				}				
			});
			$(".licont2 li").on("click",function(e){
				e.stopPropagation();
				$(".licont2 ul").stop().animate({"left":"100%"},300);
				$(".zhuanye").slideUp();
				$(".list_1 li").removeClass();
			});
			$(".licont li:last").off("click");
