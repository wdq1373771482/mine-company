		<script type="text/javascript">
			$(function(){
				$(".sort").hover(function(){
					$(".list_cont").slideDown(function(){
						$(".list_cont").hover(function(){
							$(".list_cont").stop();
						},function(){
							$(".list_cont").slideUp();
						});
					});
				},function(){
					$(".list_cont").slideUp();
				});
				//左边课程分类
				var sortIndex,_this;
				$(".list_cont li").hover(function(){
					sortIndex=$(this).index(),
					_this=$(this);
					_this.addClass("cur").siblings().removeClass("cur");
					$(".list_contss .list_c1").eq(sortIndex).show().siblings().hide();
					tab(_this);
				},function(){
					$(".list_cont li").removeClass("cur");
					$(".list_contss .list_c1").hide();
					tab(_this);
				});
				
				//右下切换
				$(".contt li").on("click",function(){
					var index=$(this).index();
					if(index == 1){
						$(this).addClass("cur2");
					}else{
						$(".contt li").eq(1).removeClass();
					}
					$(this).addClass("cur").siblings().removeClass("cur");
					$(".contt .c_1").eq(index).show().siblings().hide();
				});

			});
			function tab(_this){
				$(".list_contss .list_c1").hover(function(){
					$(".list_cont").stop();
					_this.addClass("cur").siblings().removeClass("cur");
					$(this).show().siblings().hide();
				},function(){
					$(".list_cont li").removeClass("cur");
					$(".list_contss .list_c1").hide();
					$(".list_cont").slideUp();
				});
			}
			
		</script>
