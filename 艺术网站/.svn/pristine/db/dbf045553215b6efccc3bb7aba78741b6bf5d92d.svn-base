		<script type="text/javascript">
			var timer=null;
			var i=0;
			$(function(){
				$(".col-md-3").mouseover(function(){
					clearInterval(timer);
				});
				$(".col-md-3").click(function(){
					$(this).stop().animate({width:"444px"},300).siblings().stop().animate({width:"242px"},300)
				});
				$(".col-md-3").mouseout(function(){
					//timer=setInterval("startMove()",2000)
				});
				
				//
				var w=document.body.clientWidth || document.documentElement.clientWidth;
				if(w < 1200){
					$(".col-md-3").off("click");
				}
			})
			function startMove(){
			  i++;
			  if(i>$(".col-md-3").length-1){i=0;}
			  $(".col-md-3").stop().eq(i).animate({width:"444px"},300).siblings().stop().animate({width:"242px"},300)
			};
		</script>

